{
    "extends": [ "google", "plugin:react/recommended", "prettier" ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "plugins": [ "react-hooks", "prettier" ],
    "rules": {
        "prettier/prettier": "warn"
    }
}